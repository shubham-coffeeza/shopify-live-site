                  {% else %}
                    {%- assign navbar_id = navbar_id | plus: 1 %}
                    <li class="site-nav__item {% if link.active %}site-nav--active{% endif %}" id="navbar-hover-{{ navbar_id }}">
                      <a href="{{ link.url }}" class="site-nav__link">
                        {{ link.title }}
